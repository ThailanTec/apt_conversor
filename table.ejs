<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Tabela</title>
</head>
<body>

    <table>
        <thead>
            <% header.forEach(col => {  %>

                <td> <%- col %>  </td>
                    
            
            <%  }) %>
        </thead>
    
    <tbody>
        <% row.forEach( row => {   %> 

            <tr>
                
              <%  row.forEach( element => { %>
                <td>    <%- element %> </td>

              <% }) %> 
            
            </tr>

        <% }) %> 
       
    </tbody>
</table>
</body>
</html>